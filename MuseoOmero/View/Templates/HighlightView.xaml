<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 xmlns:view="clr-namespace:MuseoOmero.View.TemplatesWin"
			 x:Class="MuseoOmero.View.TemplatesWin.HighlightView"
			 BindingContext="{Binding Source={RelativeSource Mode=Self}}"
			 x:DataType="view:HighlightView">

	<Button x:Name="Button"
			BackgroundColor="Transparent"
			BorderWidth="0"
			Pressed="Button_Pressed"
			Released="Button_Released"
			Clicked="Button_Clicked"
			CornerRadius="{Binding CornerRadius}">
	</Button>
</ContentView>
